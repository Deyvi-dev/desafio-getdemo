{
    "manifest_version": 3,
    "name": "getDemo extension",
    "version": "1.0",
    "description": "Grava o HTML da p√°gina e gera frames ao clicar em elementos.",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  